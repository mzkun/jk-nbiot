<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.goldcard.nbiot.common.dal.daointerface.UserDao">

<select id="checkUserFromUsmart" resultType="java.lang.Integer" parameterType="java.util.Map">
	select count(1) from csr where csrygid=#{username} and csrpwd=#{password}
</select>
<select id="checkUserFromGoldcard" resultType="java.lang.Integer" parameterType="java.util.Map">
	select count(1) from ns_user where username=#{username} and password=#{password}
</select>
</mapper>